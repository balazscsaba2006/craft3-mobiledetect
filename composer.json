{
  "name": "balazscsaba2006/mobiledetect",
  "description": "MobileDetect integration into Craft CMS 3.",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "mobiledetect",
    "mobile detect"
  ],
  "support": {
    "email": "csaba.balazs@humandirect.eu",
    "issues": "https://github.com/balazscsaba2006/craft3-mobiledetect/issues?state=open",
    "source": "https://github.com/balazscsaba2006/craft3-mobiledetect",
    "docs": "https://github.com/balazscsaba2006/craft3-mobiledetect"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Human Direct",
      "homepage": "https://humandirect.eu"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0",
    "mobiledetect/mobiledetectlib": "^2.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "humandirect\\mobiledetect\\": "src/"
    }
