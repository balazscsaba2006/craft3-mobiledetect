  "extra": {
    "name": "MobileDetect",
    "handle": "mobiledetect",
    "hasCpSettings": false,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/balazscsaba2006/craft3-mobiledetect/master/CHANGELOG.md",
    "class": "humandirect\\mobiledetect\\MobileDetect"
